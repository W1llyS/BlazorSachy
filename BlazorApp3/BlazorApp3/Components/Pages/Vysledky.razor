@page "/Vysledky"
@using BlazorApp3.Data.Classes
@using BlazorApp3.Data.Services
@inject vysledkyServices vysledkyServices

<PageTitle>Výsledky</PageTitle>



@if (vysledekList == null)
{
    <p><em>Loading...</em></p>
}
else
{
    foreach (var V in vysledekList)
    {
        <div>
            <p>@V.IdVysledky --- @V.IdTurnaj --- @V.IB --- @V.IC --- @V.Vysledek --- @V.Sb</p>
        </div>
    }
}

@code {
    private List<Vysledek1>? vysledekList;

    protected override async Task OnInitializedAsync()
    {
        vysledekList = await vysledkyServices.GetVysledek1sAsync(); // Ensure you have the 'Async' suffix and parentheses
        

    }
}
